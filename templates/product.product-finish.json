    "section_BiiXHq": {
      "type": "section",
      "blocks": {
        "ai_gen_block_5390ed2_CErkWk": {
          "type": "ai_gen_block_5390ed2",
          "name": "Split Showcase",
          "settings": {
            "layout_order": "row",
